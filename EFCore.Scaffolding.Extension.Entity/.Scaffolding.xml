<?xml version="1.0" encoding="utf-16"?>
<config>
  <entity Name="Class" TableName="class" Summary="班级" PrimaryKey="id">
    <property Name="Id" ValueGenerated="OnAdd" ColumnName="id" Summary="主键" />
    <property Name="Name" ColumnName="name" Summary="名称" />
    <property Name="HeadTeacherId" ColumnName="head_teacher_id" Summary="班主任ID" />
    <property Name="CreateTime" ColumnName="create_time" Summary="创建时间" />
    <property Name="UpdateTime" ColumnName="update_time" Summary="更新时间" />
    <property Name="Grade" ColumnName="grade" Summary="年级" />
    <property Name="Location" ColumnName="location" Summary="地址" />
    <property Name="IsDeleted" ColumnName="is_deleted" Summary="是否已删除" />
  </entity>
  <entity Name="Course" TableName="course" Summary="课程" PrimaryKey="id">
    <property Name="Id" ValueGenerated="OnAdd" ColumnName="id" Summary="主键" />
    <property Name="Name" ColumnName="name" Summary="名称" />
    <property Name="TeacherId" ColumnName="teacher_id" Summary="教师ID" />
    <property Name="CreateTime" ColumnName="create_time" Summary="创建时间" />
    <property Name="UpdateTime" ColumnName="update_time" Summary="更新时间" />
    <property Name="IsDeleted" ColumnName="is_deleted" Converter="BoolToZeroOne" CSharpType="bool" Summary="是否已删除" />
  </entity>
  <entity Name="CourseScore" TableName="course_score" PrimaryKey="id">
    <property Name="Id" ValueGenerated="OnAdd" ColumnName="id" Summary="主键" />
    <property Name="Score" ColumnName="score" Summary="分数" />
    <property Name="StudentId" ColumnName="student_id" Summary="学生ID" />
    <property Name="CourseId" ColumnName="course_id" Summary="课程ID" />
    <property Name="CreateTime" ColumnName="create_time" Summary="创建时间" />
    <property Name="UpdateTime" ColumnName="update_time" Summary="更新时间" />
  </entity>
  <entity Name="Log" TableName="log" Summary="日志" PrimaryKey="identifier">
    <property Name="Identifier" DefaultValueSql="(newid())" ColumnName="identifier" Summary="主键" />
    <property Name="Message" ColumnName="message" Summary="日志内容" />
    <property Name="CreateTime" ColumnName="create_time" Summary="创建时间" />
    <property Name="UpdateTimeTicks" ColumnName="update_time_ticks" Converter="DateTimeToTicks" CSharpType="DateTime?" Summary="创建时间" />
    <property Name="Url" ColumnName="url" Converter="UriToString" CSharpType="Uri" Summary="链接" />
  </entity>
  <entity Name="Student" TableName="student" Summary="学生" PrimaryKey="id">
    <property Name="Id" ValueGenerated="OnAdd" ColumnName="id" Summary="主键" />
    <property Name="Name" ColumnName="name" Summary="名称" />
    <property Name="Sex" ColumnName="sex" Converter="EnumToString" CSharpType="SexEnum" Summary="性别" />
    <property Name="ClassId" ColumnName="class_id" Summary="班级ID" />
    <property Name="CreateTime" ColumnName="create_time" Summary="创建时间" />
    <property Name="UpdateTime" ColumnName="update_time" Summary="更新时间" />
    <property Name="Address" ColumnName="address" Summary="地址" />
    <property Name="Mobile" ColumnName="mobile" Summary="联系电话" />
    <property Name="IsDeleted" ColumnName="is_deleted" Converter="BoolToString" CSharpType="bool" Summary="是否已删除" />
  </entity>
  <entity Name="Teacher" TableName="teacher" Summary="老师" PrimaryKey="id">
    <property Name="Id" ValueGenerated="OnAdd" ColumnName="id" Summary="主键" />
    <property Name="Name" ColumnName="name" Summary="名称" />
    <property Name="Sex" ColumnName="sex" Converter="EnumToString" CSharpType="SexEnum" Summary="性别" />
    <property Name="CreateTime" ColumnName="create_time" Summary="创建时间" />
    <property Name="UpdateTime" ColumnName="update_time" Summary="更新时间" />
  </entity>
  <entity Name="TeacherCourseMapping" TableName="teacher_course_mapping" PrimaryKey="id">
    <property Name="Id" ValueGenerated="OnAdd" ColumnName="id" Summary="主键" />
    <property Name="CourseId" ColumnName="course_id" Summary="课程ID" />
    <property Name="TeacherId" ColumnName="teacher_id" Summary="教师ID" />
    <property Name="CreateTime" ColumnName="create_time" Summary="创建时间" />
    <property Name="UpdateTime" ColumnName="update_time" Summary="更新时间" />
  </entity>
  <entity Name="VLog" ViewName="v_log" Summary="日志视图">
    <property Name="Identifier" ColumnName="identifier" Summary="主键" />
    <property Name="Message" ColumnName="message" Summary="日志内容" />
    <property Name="CreateTime" ColumnName="create_time" Summary="创建时间" />
    <property Name="UpdateTimeTicks" ColumnName="update_time_ticks" Converter="DateTimeToTicks" CSharpType="DateTime?" Summary="创建时间" />
    <property Name="Url" ColumnName="url" Converter="UriToString" CSharpType="Uri" Summary="链接" />
  </entity>
  <namespace Name="SexEnum">EFCore.Scaffolding.Extension.Entity.Enums</namespace>
</config>